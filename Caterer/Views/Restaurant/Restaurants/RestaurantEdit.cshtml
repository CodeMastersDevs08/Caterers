@model Caterer.Models.Restaurant
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_CaterersDashboard.cshtml";
}
<style>
    .nav-pills .nav-item {
        margin-bottom: 10px;
        margin-right: 10px;
    }

    #tabs-menagment-main {
        background-color: rgb(99, 184, 171);
        transition: background-color 0.3s ease;
        color: #000;
        margin-right: 10PX;
    }

    #res_menagment {
        list-style: none;
        padding: 0;
        margin: 0;
        border-radius: 8px;
    }

        #res_menagment li {
            margin-bottom: 1px;
        }

            #res_menagment li a {
                text-decoration: none;
                color: #333;
                padding: 8px 16px;
                display: block;
                transition: background-color 0.3s ease;
            }

                #res_menagment li a:hover {
                    background-color: wheat;
                    color: #000;
                }
</style>

<div class="container-fluid py-6">
    <div class="pb-3 pt-4 pt-md-6">
        <div class="container-fluid ">
            <div class="nav-wrapper ">
                <ul class="nav nav-pills nav-fill flex-column flex-md-row " id="res_menagment" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link mb-sm-3 mb-md-0" id="tabs-menagment-main" onclick="toggleSection('management')"><i class="ni ni-badge mr-2"></i>  Restaurant Management</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link mb-sm-3 mb-md-0" id="tabs-menagment-main" onclick="toggleSection('settings')"><i class="ni ni-money-coins mr-2"></i>  Settings</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link mb-sm-3 mb-md-0" id="tabs-menagment-main" onclick="toggleSection('hours')"><i class="ni ni-time-alarm mr-2"></i>  Working Hours</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link mb-sm-3 mb-md-0 " id="tabs-menagment-main" onclick="toggleSection('location')"><i class="ni ni-square-pin mr-2"></i>  Location</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link mb-sm-3 mb-md-0" id="tabs-menagment-main" onclick="toggleSection('financeContainer')"><i class="ni ni-money-coins mr-2"></i>  Finance</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link mb-sm-3 mb-md-0" id="tabs-menagment-main" onclick="toggleSection('planContainer')"><i class="ni ni-money-coins mr-2"></i>  Plans</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header pb-0">
                    <div class="d-flex justify-content-between align-items-center">
                        <h5>Restaurant</h5>
                        <div class="d-flex">
                            <div class="ms-auto">
                                <a class="btn btn-primary text-right btn-sm ms-auto mb-2" asp-controller="Restaurants" asp-action="RestaurantList">Back to list</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <form asp-action="RestaurantEdit" method="post" enctype="multipart/form-data">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <input type="hidden" asp-for="RestaurantId" />
                            @*     @RESTAURANT MANAGEMENT START@*@
                            <input asp-for="ResId" type="hidden" value="0" class="form-control" />
                            <div class="row" id="management">
                                <div class="col-12">
                                    <div class="card mb-4">
                                        <div class="card-header pb-0">
                                            <div class="row">
                                                <div class="col-md-12 col-lg-8">
                                                    <h5>Restaurant Information</h5>
                                                </div>
                                                <div class="col-md-12 col-lg-4">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="form-group" style="display:none">
                                                    <label asp-for="Role" class="control-label">Role</label>
                                                    <input asp-for="Role" class="form-control" value="Restaurant" />
                                                    <span asp-validation-for="Role" class="text-danger"></span>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label asp-for="RestaurantName" for="example-text-input" class="form-control-label">Restaurant Name</label>
                                                        <input asp-for="RestaurantName" class="form-control" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="Minimum" class="form-control-label">Minimum order</label>
                                                        <input class="form-control" asp-for="Minimum" type="text" placeholder="Please Enter Slug">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="Description" class="form-control-label">Restaurant description</label>
                                                        <input class="form-control" asp-for="Description" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="TakeawayMin" class="form-control-label">Takeaway Minimum order</label>
                                                        <input class="form-control" asp-for="TakeawayMin" type="text" placeholder="Please Enter Slug">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="Address" class="form-control-label">Restaurant address</label>
                                                        <input class="form-control" asp-for="Address" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="Minutes" class="form-control-label">Minutes</label>
                                                        <Select asp-for="Minutes" class="form-control">
                                                            <option disabled>Select Minutes</option>
                                                            <option>1</option>
                                                            <option>2</option>
                                                            <option>3</option>
                                                            <option>4</option>
                                                            <option>5</option>
                                                            <option>6</option>
                                                            <option>7</option>
                                                            <option>8</option>
                                                            <option>9</option>
                                                            <option>10</option>
                                                        </Select>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="OwnerPhone" class="form-control-label">Restaurant phone</label>
                                                        <input class="form-control" asp-for="OwnerPhone" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label asp-for="Delivery" for="example-text-input" class="form-control-label">Delivery Charges</label>
                                                        <input asp-for="Delivery" class="form-control" placeholder="Please Enter Delivery Charge">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label asp-for="Buffet" for="example-text-input" class="form-control-label">Buffet Setup Charges</label>
                                                        <input asp-for="Buffet" class="form-control" placeholder="Please Enter Buffet Setup Charge">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="TaxId" class="form-control-label">Default tax value</label>
                                                        <select asp-for="TaxId" asp-items="new SelectList(Model.TaxName ?? new List<Tax>(), nameof(Tax.TaxId), nameof(Tax.TaxPercentage))" class="form-control pro-edt-select form-control-primary">
                                                            <option>Select Tax</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-6" style="display:none;">
                                                    <div class="form-check">
                                                        <label class="custom-control-label">Active</label>
                                                        <input class="form-check-input" type="checkbox" asp-for="Active" id="fcustomCheck1">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="Fee" class="form-control-label">Platform Fee percent</label>
                                                        <input class="form-control" asp-for="Fee" type="number" placeholder="Please Select Tax">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="StaticFee" class="form-control-label">Platform Static fee </label>
                                                        <input class="form-control" asp-for="StaticFee" type="number" placeholder="30">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="AutoRejectWaitTime" class="form-control-label">Auto Reject Waiting time  </label>
                                                        <input class="form-control" asp-for="AutoRejectWaitTime" type="" placeholder="₹0">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="StripeFee" class="form-control-label">Stripe Fee</label>
                                                        <input class="form-control" asp-for="StripeFee" type="number" placeholder="Please Select Tax">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="StripeStaticFee" class="form-control-label">Stripe Static fee </label>
                                                        <input class="form-control" asp-for="StripeStaticFee" type="number" placeholder="30">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="QRPaymentWaitTime" class="form-control-label">Qr Payment Waiting time  </label>
                                                        <input class="form-control" asp-for="QRPaymentWaitTime" type="" placeholder="₹0">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="PayNowFee" class="form-control-label">Paynow Fee</label>
                                                        <input class="form-control" asp-for="PayNowFee" type="number" placeholder="Please Select Tax">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="PayNowStaticFee" class="form-control-label">Paynow Static fee </label>
                                                        <input class="form-control" asp-for="PayNowStaticFee" type="number" placeholder="30">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="AverageOrderPrepareTimeInMinutes" class="form-control-label">Average order prepare time in minutes  </label>
                                                        <select asp-for="AverageOrderPrepareTimeInMinutes" class="form-control">
                                                            <option disabled>Selected Average order prepare time In minutes</option>
                                                            <option>0</option>
                                                            <option>5</option>
                                                            <option>10</option>
                                                            <option>15</option>
                                                            <option>20</option>
                                                            <option>25</option>
                                                            <option>30</option>
                                                            <option>35</option>
                                                            <option>40</option>
                                                            <option>45</option>
                                                            <option>50</option>
                                                            <option>55</option>
                                                            <option>60</option>
                                                            <option>65</option>
                                                            <option>70</option>
                                                            <option>75</option>
                                                            <option>85</option>
                                                            <option>90</option>
                                                            <option>95</option>
                                                            <option>100</option>
                                                            <option>105</option>
                                                            <option>110</option>
                                                            <option>115</option>
                                                            <option>120</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label asp-for="TimeSlotSeparatedInMinutes" class="form-control-label">Time slots separated in minutes</label>
                                                        <select asp-for="TimeSlotSeparatedInMinutes" class="form-control">
                                                            <option disabled>Selected Time slot separated in minutes</option>
                                                            <option>0</option>
                                                            <option>5</option>
                                                            <option>10</option>
                                                            <option>15</option>
                                                            <option>20</option>
                                                            <option>25</option>
                                                            <option>30</option>
                                                            <option>35</option>
                                                            <option>40</option>
                                                            <option>45</option>
                                                            <option>50</option>
                                                            <option>55</option>
                                                            <option>60</option>
                                                            <option>65</option>
                                                            <option>70</option>
                                                            <option>75</option>
                                                            <option>85</option>
                                                            <option>90</option>
                                                            <option>95</option>
                                                            <option>100</option>
                                                            <option>105</option>
                                                            <option>110</option>
                                                            <option>115</option>
                                                            <option>120</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label asp-for="TimeSlotSeparatedInMinutes" class="form-control-label">Opening time interval in minutes</label>
                                                        <input class="form-control" asp-for="TimeSlotSeparatedInMinutes" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label asp-for="DeliveryPreparationTime" class="form-control-label">Delivery preparation time</label>
                                                        <input class="form-control" asp-for="DeliveryPreparationTime" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="ServiceTax" class="form-control-label">Service Tax</label>
                                                        <input class="form-control" asp-for="ServiceTax" type="text" placeholder="Please Enter Name">

                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="StripeServiceTax" class="form-control-label">Stripe Service Charge</label>
                                                        <input class="form-control" asp-for="StripeServiceTax" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="PayNowServiceTax" class="form-control-label">Paynow Service Charge</label>
                                                        <input class="form-control" asp-for="PayNowServiceTax" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="ServiceTaxDineIn" class="form-control-label">Dine-in Service Tax</label>
                                                        <input class="form-control" asp-for="ServiceTaxDineIn" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="FreeDeliveryCost" class="form-control-label">Free Delivery Cost</label>
                                                        <input class="form-control" asp-for="FreeDeliveryCost" type="text" placeholder="Please Enter Name">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="Currency" class="form-control-label">Currency</label>
                                                        <input class="form-control" asp-for="Currency" type="text" placeholder="Please Enter Slug">
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label for="example-text-input" asp-for="DefaultLanguage" class="form-control-label">Default Language  </label>
                                                        <input class="form-control" type="text" asp-for="DefaultLanguage" placeholder="Please Enter Slug">
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label asp-for="Logo" class="control-label">Restaurant Image</label> <br />
                                                        <label><span>JPEG 590 x 400 recommended</span></label><br />
                                                        <input asp-for="Logo" type="hidden" class="form-control" />
                                                        @if (!string.IsNullOrEmpty(Model.Logo))
                                                        {
                                                            <img id="logoPreview" src="~/Images/RestaurantLogo/@Model.Logo" style="width:100px; border-radius:10px; height:80px" />
                                                        }
                                                        <br />
                                                        <br />
                                                        <label class="btn btn-default">
                                                            Change Image
                                                            <input asp-for="Logo" type="file" name="ProductImage" accept=".png,.jpeg,.jpg" class="d-none" onchange="previewImage(this, 'logoPreview')" />
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label asp-for="Cover" class="control-label">Restaurant Cover Image</label> <br />
                                                        <label><span>JPEG 2000 x 1000 recommended</span></label><br />
                                                        <input asp-for="Cover" type="hidden" class="form-control" />
                                                        @if (!string.IsNullOrEmpty(Model.Cover))
                                                        {
                                                            <img id="coverPreview" src="/Images/RestaurantCover/@Model.Cover" style="width:180px; border-radius:10px; height:80px" />
                                                        }
                                                        <br />
                                                        <br />
                                                        <label class="btn btn-default">
                                                            Change Image
                                                            <input asp-for="Cover" type="file" name="coverimg" accept=".png,.jpeg,.jpg" class="d-none" onchange="previewImage(this, 'coverPreview')" />
                                                        </label>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <h5>OWNER INFORMATION</h5>
                                                    <hr class="horizontal dark">
                                                    <div class="col-md-12">
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="form-control-label">  Name</label>
                                                            <input class="form-control" disabled asp-for="OwnerName" type="text">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="form-control-label">  Email</label>
                                                            <input class="form-control" disabled asp-for="OwnerEmail" type="text">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="form-control-label">  Phone</label>
                                                            <input class="form-control" disabled asp-for="OwnerPhone" type="text">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="example-text-input" class="form-control-label">Password</label>
                                                            <input asp-for="Password" class="form-control" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*  RESTAURANT MANAGEMENT End*@

                            @*SETTINGS MANAGEMENT START*@
                            <div class="row" id="settings">
                                <div class="col-12">
                                    <div class="card mb-4">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-md-6" style="display:none;">
                                                    <div class="form-check form-switch">
                                                        <input class="form-check-input float-end" type="checkbox" asp-for="IsFeatured" id="flexSwitchCheckDefault">
                                                        <label class="form-check-label"> @Html.DisplayNameFor(model => model.IsFeatured)  </label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanAutoReject" type="checkbox" id="autoRejectToggle">
                                                        <label for="autoRejectToggle" class="custom-control-label">Enable Auto Reject</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanAutoAccept" type="checkbox" id="autoAcceptToggle">
                                                        <label for="autoAcceptToggle" class="custom-control-label">Enable Auto Accept</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanPickup" type="checkbox" id="pickupToggle">
                                                        <label for="pickupToggle" class="custom-control-label">Pickup</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanDeliver" type="checkbox" id="deliveryToggle">
                                                        <label for="deliveryToggle" class="custom-control-label">Delivery</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanFreeDeliver" type="checkbox" id="freeDeliveryToggle">
                                                        <label for="freeDeliveryToggle" class="custom-control-label">Free Delivery</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanPreorder" type="checkbox" id="preorderToggle">
                                                        <label for="preorderToggle" class="custom-control-label">Pre Order</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="Disableordering" type="checkbox" id="disableOrderingToggle">
                                                        <label for="disableOrderingToggle" class="custom-control-label">Disable Ordering</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" type="checkbox" asp-for="CanDineIn" id="dineInToggle">
                                                        <label for="dineInToggle" class="custom-control-label">Dine In</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" type="checkbox" asp-for="DisableCOD" id="disableCODToggle">
                                                        <label for="disableCODToggle" class="custom-control-label">Disable Cash On Delivery</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="DisableCardpayment" type="checkbox" id="disableCardPaymentToggle">
                                                        <label for="disableCardPaymentToggle" class="custom-control-label">Disable Card Payment</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="DisablePayNow" type="checkbox" id="disablePayNowToggle">
                                                        <label for="disablePayNowToggle" class="custom-control-label">Disable Paynow</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="CanSelfDelivery" type="checkbox" id="selfDeliveryToggle">
                                                        <label for="selfDeliveryToggle" class="custom-control-label">Self Delivery</label>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="custom-control custom-checkbox">
                                                        <input class="custom-control-input" asp-for="Halal" type="checkbox" id="halalToggle">
                                                        <label for="halalToggle" class="custom-control-label">Halal</label>
                                                    </div>
